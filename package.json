{
  "name": "appcelerator-titanium",
  "main": "./lib/index",
  "version": "1.10.1",
  "description": "Appcelerator Titanium build tools and UI for Atom",
  "repository": "https://github.com/appcelerator/atom-appcelerator-titanium",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">1.21.0"
  },
  "keywords": [
    "axway",
    "appcelerator",
    "titanium",
    "alloy",
    "mobile",
    "javascript",
    "ios",
    "android",
    "windows"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "provideAutoComplete"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": "npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "atomTestRunner": "./spec/test-runner.js",
  "dependencies": {
    "etch": "^0.12.6",
    "find": "^0.3.0",
    "mkdirp": "^1.0.4",
    "semver": "^7.3.2",
    "sinon": "^9.0.2",
    "sudo-prompt": "^9.2.1",
    "titanium-editor-commons": "^0.5.0",
    "underscore": "^1.10.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@atom/mocha-test-runner": "^1.5.1",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@seadub/danger-plugin-dependencies": "^0.1.0",
    "@seadub/danger-plugin-eslint": "^1.0.1",
    "@seadub/danger-plugin-junit": "^0.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "danger": "^10.2.1",
    "eslint": "^6.8.0",
    "eslint-config-axway": "^4.7.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha-jenkins-reporter": "^0.4.3",
    "standard-version": "^8.0.0"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint --ext .js,.jsx .",
    "release": "standard-version",
    "test": "apm --version && apm test"
  },
  "standard-version": {
    "message": "chore(release): %s [ci skip]"
  }
}
